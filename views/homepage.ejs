  <ul class="downloadTab">
    <div id="television">
      <div>
        <h4 class="first-margin">Television</h4>
        <hr style="width: 93%;" />
      </div>
      <ul>
        <% products.forEach(function(product){%> <% if(product.brand ==
        "Sony"||product.brand == "sony"){ %>
        <li>
          <img class="tv-class" src="/<%=product.image%>" alt="" />
          <div class="imgText">
            <div class="imgtextHead">
              <%= product.brand %> <%= product.name %>
            </div>
            <div class="imgtextHead">
              Rs <%= product.price %>
            </div>
            <div class="imgtextRow">
              <div class="rowView">
                <button class="btn btn-default">
                  <a href="/products/<%= product._id %>/view"
                    >View
                    <span><i class="fa fa-eye" aria-hidden="true"></i></span
                  ></a>
                </button>
              </div>
              <div class="rowDownload">
                <button class="btn btn-default">
                  <a href="/products/<%= product._id %>/add-to-cart"
                    >Add to cart
                    <span
                      ><i
                        class="fa fa-shopping-cart"
                        aria-hidden="true"
                      ></i></span
                  ></a>
                </button>
              </div>
            </div>
          </div>
        </li>
        <%}%> <%})%>
      </ul>
    </div>

